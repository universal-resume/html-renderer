{
    "name": "@universal-resume/html-renderer",
    "version": "2.1.0",
    "description": "A TypeScript library for rendering resumes to HTML with customizable themes and templates",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "types": "dist/types/index.d.ts",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "serve": "vite",
        "build": "vite build",
        "check": "biome check .",
        "format": "biome format --write .",
        "lint": "biome lint --write .",
        "prepublishOnly": "npm run build"
    },
    "author": "Universal Resume",
    "license": "MIT",
    "devDependencies": {
        "@biomejs/biome": "^2.1.2",
        "typescript": "^5.0.0",
        "vite": "^6.3.5",
        "vite-plugin-dts": "^4.5.4"
    },
    "dependencies": {
        "@universal-resume/json-examples": "2.0.1",
        "@universal-resume/ts-schema": "2.1.0",
        "effect": "^3.17.2"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}